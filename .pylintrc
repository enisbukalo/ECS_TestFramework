[MASTER]
ignore=CVS
persistent=yes
load-plugins=

[MESSAGES CONTROL]
disable=C0111,C0103,C0303,W0311,R0903,W0621,C0301,W0622,C0114,C0115,C0116

[FORMAT]
max-line-length=251
indent-string='    '

[BASIC]
good-names=i,j,k,ex,Run,_,id,dt

[MISCELLANEOUS]
notes=FIXME,XXX,TODO

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes